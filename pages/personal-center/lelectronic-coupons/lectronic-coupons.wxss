page{
  background-color: #ECECEC;
  width: 100%;
}

.lectronic-wrap{
  width: 100%;
  height: 232rpx;
  background-color: white;
  margin-bottom: 20rpx;
}

.lectronic-top{
  width: 100%;
  height: 168rpx;
  display: flex;
  justify-content: space-between;
  border: 1px dotted #E0E0E0;
  box-sizing: border-box;
}

.lectronic-left{
  width: 80%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.lectronic-right{
  width: 20%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #555555;
  font-size: 28rpx;
  letter-spacing: 1rpx;
}

.lectronic-images{
  width: 102rpx;
  height: 100rpx;
}

.lectronic-wrap-im{
  width: 24%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.lectronic-text{
  width: 74%;
  height: 168rpx;
  letter-spacing: 2rpx;
}

.lectronic-reel{
  display: block;
  color: #191919;
  font-size: 46rpx;
  padding-top: 30rpx;
}

.lectronic-limit{
  color: #555555;
  font-size: 30rpx;
}

.restrict-time{
  width: 100%;
  height: 62rpx;
  color: #B1B1B1;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.restrict-Images{
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
}

.restrict-caution{
  padding-left: 30rpx;
}

.restric-page-down{
  padding-right: 30rpx;
}

.restric-page-text{
  padding-right: 15rpx;
  vertical-align: middle;
}

.qr-code{
  width: 100%;
  height: 394rpx;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-flow: column;
  margin-bottom: 20rpx;
}

.qr-code-images{
  width: 270rpx;
  height: 270rpx;
}

.qr-code-text{
  color: #555555;
  font-size: 30rpx;
}

.qr-code-matter{
  padding-left: 20rpx;
  letter-spacing: 1px;
}

.order-for-goods{
  width: 100%;
  height: 380rpx;
  background-color: white;
  display: flex;
  justify-content: center;
}

.order-for-nei{
  width: 90%;
  height: 380rpx;
}

.order-for-text{
  color: #B1B1B1;
  font-size: 24rpx;
  height: 62rpx;
  border-bottom: 1px dotted #E0E0E0;
  line-height: 62rpx;
}

.order-for-colour{
  color: #555555;
  font-size: 24rpx;
  height: 62rpx;
  line-height: 62rpx;
  display: flex;
  justify-content: space-between;
}

.warm-prompt{
  width: 100%;
  height: 100rpx;
  color: #B1B1B1;
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}