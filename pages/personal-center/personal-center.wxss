page{
  background-color: #EBEBEB;
}

.centreWrap{
  background-color: white;
  height: 470rpx;
  margin-bottom: 20rpx;
}

.centreTopName{
  height: 300rpx;
  text-align: center;
  padding-top: 60rpx;
}
.chname{
  margin: 0 125rpx;
  width: 500rpx;
  height: 50rpx;
  border: 1rpx solid #b1b1b1;
}
.inputname{
  float: left;
}
.centreImages{
  width: 154rpx;
  height: 154rpx;
  border-radius: 50%;
  border: 1px solid #E0E0E0;
}

.centreText{
  color: #191919;
  font-size: 36rpx;
  font-weight: 600; 
  font-family: Adobe Heiti Std R;
}

.halving-line{
  width: 1rpx;
  height: 66rpx;
  padding-top: 10rpx;
}

.messageCase{
  display: flex;
  justify-content: space-around;
  color: #191919;
}

.messageTop{
  display: block;
  padding-bottom: 12rpx;
}

.messageWrap{
  width: 33%;
  text-align: center;
  font-size: 30rpx;
}

.myList{
  height: 100rpx;
  background-color: white;
  padding-left: 26rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #E0E0E0;
}

.myListImages{
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
}

.myListArrows{
  width: 12rpx;
  height: 22rpx;
  padding-right: 26rpx;
}

.mylistText{
  color: #191919;
  font-size: 30rpx;
  padding-left: 20rpx;
  vertical-align: middle;
}

.myList-single{
  width: 100%;
  display: flex;
  align-items: center; 
  justify-content: space-between;
  height: 90rpx;
}

.mylistWrap{
  padding-bottom: 20rpx;
}

.customer-service{
  width: 58rpx;
  height: 58rpx;
  background-color: #FC5E2D;
  border-radius: 50%;
  text-align: center;
  line-height: 70rpx;
  margin-right: 30rpx;
  margin-top: 10rpx;
}

.wicket{
  margin-top: 11rpx;
  width: 50rpx;
  height: 100rpx;
}

.telephone-Wrap{
  width: 56%;
  text-align: right;
}

.telephone{
  width: 58rpx;
  height: 58rpx;
  padding-top: 45rpx;
}
