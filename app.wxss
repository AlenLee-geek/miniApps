/**app.wxss**/
.page_row{
  display: block;
}

.search{
  width: 100%;
  background: #fc5e2d;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.search_arr {
  width: 92%;
  background: white;
  height: 60rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}

.searchcion {
  margin: 10rpx 10rpx 10rpx 10rpx;
  position: absolute;
  left: 8%;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}

.search_arr input {
  margin-left: 90rpx;
  height: 60rpx;
  border-radius: 5px;
}

.inputs {
  display: block;
  height: 1.4rem;
  text-overflow: clip;
  overflow: hidden;
  white-space: nowrap;
  font-family: UICTFontTextStyleBody;
  min-height: 1.4rem;
  font-size: 13px;
  letter-spacing: 4rpx;
}

